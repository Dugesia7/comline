//other functions

//dictionary comparison
//if word1 is earlier, return 1
//if word2 is earlier, return -1
//if equal, return 0
int dictcomp(char* word1,char* word2){
	int i;
	for(i=0;i>=0;i++){
		if(word1[i]<word2[i])
			i=-1;
		else if(word1[i]>word2[i])
			i=-3;
		else if(word1[i]=='\0')
                        i=-2;
	}
	return i+2

}
